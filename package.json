{
  "name": "scullog",
  "version": "0.2.0",
  "description": "Scullog based on Node.js and Angular.js",
  "main": "./server/index.js",
  "scripts": {
    "prebuild": "rm -rf dist",
    "start": "node --harmony ./server/index.js",
    "build": "parcel build client/index.html",
    "posttest": "node --harmony ./e2e/conf/posttest.js",
    "preupdate-webdriver": "yarn install && yarn build",
    "update-webdriver": "webdriver-manager update",
    "pretest": "yarn run update-webdriver && node --harmony ./e2e/conf/pretest.js",
    "test": "protractor ./e2e/protractor.conf.js",
    "preE2ETest": "yarn run-script posttest && yarn run-script pretest"
  },
  "bin": {
    "scullog": "../scullog/bin/scullog"
  },
  "dependencies": {
    "ace": "^1.3.0",
    "ace-builds": "^1.3.3",
    "adm-zip": "^0.4.7",
    "angular": "1.5.8",
    "angular-loading-bar": "^0.9.0",
    "angular-toastr": "^2.1.1",
    "angular-ui-ace": "^0.2.3",
    "angular-ui-bootstrap": "^2.5.6",
    "angularjs-scroll-glue": "^2.2.0",
    "bootstrap": "4.0.0-alpha.3",
    "circular-buffer": "1.0.1",
    "co": "^4.6.0",
    "co-busboy": "1.3.1",
    "co-child-process": "^1.1.0",
    "co-exec": "^1.1.0",
    "co-fs": "^1.2.0",
    "co-fs-extra": "^1.1.0",
    "co-views": "^2.1.0",
    "cross-spawn": "^4.0.0",
    "ejs": "^2.5.1",
    "file-saver": "^1.3.8",
    "font-awesome": "4.6.3",
    "got": "^8.3.0",
    "jquery": "^3.3.1",
    "js": "^0.1.0",
    "jsonwebtoken": "^8.2.0",
    "koa": "^1.0.0",
    "koa-bodyparser": "^2.0.1",
    "koa-compress": "^1.0.9",
    "koa-cors": "0.0.16",
    "koa-morgan": "^0.4.0",
    "koa-mount": "^1.3.0",
    "koa-router": "^5.1.2",
    "koa-static": "^2.0.0",
    "logrotate-stream": "^0.2.5",
    "mime-types": "2.1.11",
    "ng-file-upload": "^12.2.13",
    "socket.io": "1.4.8",
    "tracer": "^0.8.0",
    "yargs": "^5.0.0"
  },
  "devDependencies": {
    "parcel-bundler": "^1.9.7",
    "webdriver-manager": "^12.1.0",
    "cssnano": "^4.0.5",
    "jasmine": "^2.8.0",
    "protractor": "^5.1.1",
    "typescript": "^2.2.1"
  },
  "optionalDependencies": {
    "node-mac": "^0.1.4",
    "node-windows": "^0.1.11"
  },
  "os": [
    "darwin",
    "win32",
    "linux"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sanketbajoria/scullog.git"
  },
  "keywords": [
    "files",
    "manager",
    "koa",
    "angular",
    "web",
    "server"
  ],
  "author": {
    "name": "Sanket Bajoria",
    "email": "bajoriasanket@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sanketbajoria/scullog/issues"
  },
  "homepage": "https://github.com/sanketbajoria/scullog#readme"
}
